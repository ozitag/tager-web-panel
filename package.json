{
  "name": "@tager/web-panel",
  "version": "0.1.7",
  "author": "Ivan Saranchonkau",
  "license": "MIT",
  "main": "dist/index.js",
  "umd:main": "dist/web-core.umd.production.js",
  "module": "dist/web-core.esm.js",
  "typings": "dist/main.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "start": "next dev",
    "build": "cross-env NODE_ENV=production tsdx build --format=cjs,esm --tsconfig=tsconfig.tsdx.json --entry ./src/main.ts",
    "test": "tsdx test",
    "lint": "eslint --fix ./src/**/*.{ts,tsx}",
    "prebuild": "rimraf dist",
    "prepare": "yarn build"
  },
  "dependencies": {},
  "devDependencies": {
    "@rollup/plugin-commonjs": "^15.0.0",
    "@tager/web-core": "^0.3.1",
    "@types/node": "12.x",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "2.x",
    "@typescript-eslint/parser": "2.x",
    "babel-eslint": "10.x",
    "cross-env": "^7.0.2",
    "eslint": "6.x",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "4.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "2.x",
    "husky": "^4.3.0",
    "next": "^9.5.3",
    "normalize.css": "^8.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rimraf": "^3.0.2",
    "styled-components": "^5.2.0",
    "tsdx": "^0.13.3",
    "tslib": "^2.0.1",
    "typescript": "3.9.7"
  },
  "peerDependencies": {
    "@tager/web-core": "^0.3.0",
    "next": "^9.5.3",
    "react": "^16.13.1",
    "styled-components": "^5.2.0"
  },
  "resolutions": {
    "prettier": "^2.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "post-commit": "git update-index --again"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
